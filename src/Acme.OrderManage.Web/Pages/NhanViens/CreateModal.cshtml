@page
@using Acme.OrderManage.Localization
@using Acme.OrderManage.Web.Pages.NhanViens
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<OrderManageResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="NhanVien" asp-page="/NhanViens/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NhanVienMoi"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>

